{
  "name": "Tab Recording",
  "description": "Record Audio Clips from a tab and share.",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "background": {
    "scripts": ["background.js"]
  },
  "version": "1.0",
  "minimum_chrome_version": "9",
  "manifest_version": 2,

  "permissions": [
   "activeTab"
   ]
  }